{
  "version": "2.0.0",
  "main": "main.js",
  "name": "grammarly-example-electron-with-vue",
  "displayName": "Editor SDK Vue Demo",
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "build-vue": "vue-cli-service build",
    "lint-vue": "vue-cli-service lint"
  },
  "devDependencies": {
    "electron": "^16.0.5",
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-service": "4.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "build": {
    "appId": "com.grammarly.developer.example",
    "protocols": {
      "name": "example-protocol",
      "schemes": [
        "example"
      ]
    }
  },
  "dependencies": {
    "path": "^0.12.7",
    "@grammarly/editor-sdk-vue": "^1.10.2",
    "@vue/cli-plugin-babel": "4.1.1",
    "vue": "^2.6.11"
  }
}
